{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "noEmit": true,
    "strict": false,
    "strictNullChecks": false,
    "noImplicitAny": false
  },
  "exclude": [
    "node_modules",
    ".next",
    "out",
    "build",
    "dist",
    "scripts",
    "test",
    "*.config.js",
    "*.config.ts"
  ]
}
