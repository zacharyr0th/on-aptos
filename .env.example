# Environment Variables Example
# Copy this file to .env and fill in your actual values

# Site Configuration
NEXT_PUBLIC_SITE_URL=https://yourdomain.com
NEXT_PUBLIC_APP_NAME=Your App Name
NEXT_PUBLIC_SITE_DESCRIPTION=Your app description
NEXT_PUBLIC_CORS_ORIGINS=https://yourdomain.com,https://www.yourdomain.com

# Port Configuration
PORT=3000
DEV_PORT=3000

# API Keys
CMC_API_KEY=your_coinmarketcap_api_key_here
RWA_API_KEY=your_rwa_api_key_here
APTOS_BUILD_SECRET=your_aptos_build_secret_here

# API Endpoints
RWA_API_BASE_URL=https://api.rwa.xyz/v4/assets
CMC_API_BASE_URL=https://pro-api.coinmarketcap.com/v1

# Logging Configuration
# Available levels: trace, debug, info, warn, error, fatal, silent
LOG_LEVEL=info