{
  "name": "on-aptos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3001",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start --port 3001",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "clean": "rm -rf node_modules .next .turbo dist build tmp .cache package-lock.json yarn.lock pnpm-lock.yaml && pnpm install",
    "sitemap": "next-sitemap",
    "translate-locales": "node scripts/translate-locales.js",
    "analyze:translations": "node scripts/analyze-translations.js",
    "validate:translations": "tsx scripts/validate-translations.ts",
    "fix:translations": "tsx scripts/fix-missing-translations.ts",
    "analyze": "ANALYZE=true pnpm build",
    "analyze:server": "BUNDLE_ANALYZE=server pnpm build",
    "analyze:browser": "BUNDLE_ANALYZE=browser pnpm build",
    "kill": "lsof -ti :3001 | xargs kill -9",
    "commitlint": "commitlint --edit",
    "security:scan": "gitleaks detect --source . --config .gitleaks.toml",
    "security:baseline": "detect-secrets scan --baseline .secrets.baseline --update",
    "security:audit": "detect-secrets audit .secrets.baseline",
    "hooks:install": "pre-commit install && pre-commit install --hook-type commit-msg",
    "hooks:update": "pre-commit autoupdate",
    "hooks:run": "pre-commit run --all-files",
    "validate:env": "node .env.validation.js",
    "check:security": "pnpm audit && pnpm security:scan",
    "prepare:release": "pnpm lint && pnpm test && pnpm validate:env"
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": "^3.1.3",
    "@aptos-labs/wallet-adapter-react": "^7.0.4",
    "@ducanh2912/next-pwa": "^10.2.9",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/react-window": "^1.8.8",
    "@vercel/analytics": "^1.5.0",
    "@vercel/postgres": "^0.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dotenv": "^17.2.1",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "es-abstract": "^1.24.0",
    "form-data": "^4.0.4",
    "framer-motion": "^12.23.12",
    "geist": "^1.4.2",
    "i18next": "^25.4.2",
    "i18next-http-backend": "^3.0.2",
    "input-otp": "^1.4.2",
    "json-buffer": "^3.0.1",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.541.0",
    "next": "15.5.0",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.6",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-i18next": "^15.7.2",
    "react-icons": "^5.5.0",
    "react-resizable-panels": "^3.0.5",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "recharts": "^3.1.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2",
    "vercel": "^46.0.2",
    "web-vitals": "^5.1.0",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@next/bundle-analyzer": "^15.5.0",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "24.3.0",
    "@types/react": "19.1.11",
    "@vitejs/plugin-react": "^5.0.1",
    "audit-ci": "^7.1.0",
    "eslint": "9.34.0",
    "eslint-config-next": "15.5.0",
    "eslint-config-prettier": "^10.1.8",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "sharp": "^0.34.3",
    "sort-package-json": "^3.4.0",
    "tailwindcss": "^4.1.12",
    "tw-animate-css": "^1.3.7",
    "typescript": "5.9.2",
    "vitest": "^3.2.4"
  }
}
