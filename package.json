{
  "name": "on-aptos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3001",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start --port 3001",
    "lint": "next lint",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "clean": "rm -rf node_modules .next .turbo dist build tmp .cache package-lock.json yarn.lock pnpm-lock.yaml && pnpm install",
    "sitemap": "next-sitemap",
    "translate-locales": "node scripts/translate-locales.js",
    "analyze": "ANALYZE=true pnpm build",
    "analyze:server": "BUNDLE_ANALYZE=server pnpm build",
    "analyze:browser": "BUNDLE_ANALYZE=browser pnpm build",
    "kill": "lsof -ti :3001 | xargs kill -9"
  },
  "dependencies": {
